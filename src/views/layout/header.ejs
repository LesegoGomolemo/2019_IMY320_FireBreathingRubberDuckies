<div id="top-page" class="fullscreen">
    <div class="center">
        <header class="main-header">
            <h1><span><%= firstWordTitle %></span><%= restOfTitle%></h1>
            <p><%= pageDescription %></p>
        </header>
    </div>
    <div class="scroll">
        <a href=<%= leftAddress %>>
            <div class="left-arrow color-fade" style="display: inline-block">
                <p class="next-page" style="font-size:2vh; color: #ffffff;"><%= leftPage %></p>
                <i class="fa fa-angle-double-left color-fade-icon" style="font-size:6vh;"></i>
            </div>
        </a>
        <a href=<%= downAddress %>>
            <div class="color-fade" style="display: inline-block">
                <p class="meet-team" style="font-size:2vh; color: #ffffff;"><%= thisHeading %></p>
                <br>
                <i class="fa fa-angle-double-down color-fade-icon" style="font-size:6vh;"></i>
            </div>
        </a>
        <a href=<%= rightAddress %>>
            <div class="right-arrow color-fade" style="display: inline-block">
                <p class="next-page" style="font-size:2vh; color: #ffffff;"><%= rightPage %></p>
                <i class="fa fa-angle-double-right color-fade-icon" style="font-size:6vh;"></i>
            </div>
        </a>
    </div>
</div>
